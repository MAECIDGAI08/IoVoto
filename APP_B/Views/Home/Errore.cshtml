@using Microsoft.AspNetCore.Mvc.Localization
@using Microsoft.Extensions.Localization
@using Microsoft.AspNetCore.Html;
@using Microsoft.AspNetCore.Http;
@{
    Layout = "_Layout";
}
@inject IViewLocalizer Localizer
@inject IStringLocalizer<SharedResource> Localizer
@inject Microsoft.AspNetCore.Http.IHttpContextAccessor HttpContextAccessor;
<div class="container">
    <div class="row mt-5 mb-5">
        <div class="col-sm-12 col-md-12 col-lg-12 col-xl-12">
            <div class="lookC pt-5 pl-5 pr-5 pb-5">
                <i class="fa fa-exclamation-triangle fa-4x fa-pull-left text-danger" aria-hidden="true"></i>
                <h2 class="font-weight-bold">Attenzione! Errore interno!</h2>
                <h2 class="font-weight-bold">Necessario rieffettuare l'accesso all'applicazione.</h2>
            </div>
        </div>
    </div>
    <div class="row mt-4">
        <div class="col-sm-4 col-md-4 col-lg-4 col-xl-4"></div>
        <div class="col-sm-4 col-md-4 col-lg-4 col-xl-4">
            <partial name="_ButtonAppA" />
        </div>
        <div class="col-sm-4 col-md-4 col-lg-4 col-xl-4"></div>
    </div>
</div>